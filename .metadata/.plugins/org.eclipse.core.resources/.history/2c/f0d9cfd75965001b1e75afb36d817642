package com.hithaui;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

@Component
public class Animal {
	
	@Autowired
	private Sleep sleep;
	
	public Animal() {
	}

	public Animal(Sleep sleep) {
		super();
		this.sleep = sleep;
	}

	public Sleep getSleep() {
		return sleep;
	}

	public void setSleep(Sleep sleep) {
		this.sleep = sleep;
	}
}
